package noumena.payment.gnetop;

public class GnetopOrderVO {
	// 订单号
	private String orderId;
	// 服务器号
	private String serverId;
	// 账户
	private String account;
	// 角色id
	private String userId;
	// 产品id
	private String produceId;
	// 支付金额
	private String amount;
	// 描述
	private String gamepoints;
	// 签名
	private String sign;
	// 时间戳
	private String time;

	public String getOrderId() {
		return orderId;
	}

	public void setOrderId(String orderId) {
		this.orderId = orderId;
	}

	public String getServerId() {
		return serverId;
	}

	public void setServerId(String serverId) {
		this.serverId = serverId;
	}

	public String getAccount() {
		return account;
	}

	public void setAccount(String account) {
		this.account = account;
	}

	public String getUserId() {
		return userId;
	}

	public void setUserId(String userId) {
		this.userId = userId;
	}

	public String getProduceId() {
		return produceId;
	}

	public void setProduceId(String produceId) {
		this.produceId = produceId;
	}

	public String getAmount() {
		return amount;
	}

	public void setAmount(String amount) {
		this.amount = amount;
	}

	public String getGamepoints() {
		return gamepoints;
	}

	public void setGamepoints(String gamepoints) {
		this.gamepoints = gamepoints;
	}

	public String getSign() {
		return sign;
	}

	public void setSign(String sign) {
		this.sign = sign;
	}

	public String getTime() {
		return time;
	}

	public void setTime(String time) {
		this.time = time;
	}

	@Override
	public String toString() {
		return "GnetopOrderVO [orderId=" + orderId + ", serverId=" + serverId
				+ ", account=" + account + ", userId=" + userId
				+ ", produceId=" + produceId + ", amount=" + amount
				+ ", gamepoints=" + gamepoints + ", sign=" + sign + ", time="
				+ time + "]";
	}

	public GnetopOrderVO(String orderId, String serverId, String account,
			String userId, String produceId, String amount, String gamepoints,
			String sign, String time) {
		super();
		this.orderId = orderId;
		this.serverId = serverId;
		this.account = account;
		this.userId = userId;
		this.produceId = produceId;
		this.amount = amount;
		this.gamepoints = gamepoints;
		this.sign = sign;
		this.time = time;
	}

	public GnetopOrderVO() {
		super();
	}
	

}
