	private Vector<MyCardTWParamApp> apps = new Vector<MyCardTWParamApp>();
	
	public Vector<MyCardTWParamApp> getApps()
	{
		return apps;
	}

	public void addApp(MyCardTWParamApp app)
	{
		this.getApps().add(app);
	}
	
	public void addApp(String appname, String appid, String appkey)
	{
		MyCardTWParamApp app = new MyCardTWParamApp();
		app.setAppname(appname);
		app.setAppid(appid);
		app.setAppkey(appkey);
		this.getApps().add(app);
	}
	
	public String getAppKeyById(String appid)
	{
		for (MyCardTWParamApp app : this.getApps())
		{
			if (app.getAppid().equals(appid))
			{
				return app.getAppkey();
			}
		}
		return null;
	}
