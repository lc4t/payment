package noumena.payment.gash;

public class GASHOrderResultVO
{
	public static final int ORDER_STATUS_SUCCESS	= 0;
	public static final int ORDER_STATUS_FAILURE	= 9;
	public static final int ORDER_STATUS_NOAPPID	= -1;
	
	private int status = ORDER_STATUS_FAILURE;
	private String detail = "1000";
	private String message = "unknown";
	private String appid;
	
	public int getStatus()
	{
		return status;
	}
	public void setStatus(int status)
	{
		this.status = status;
	}
	public String getDetail()
	{
		return detail;
	}
	public void setDetail(String detail)
	{
		this.detail = detail;
	}
	public String getMessage()
	{
		return message;
	}
	public void setMessage(String message)
	{
		this.message = message;
	}
	public String getAppid()
	{
		return appid;
	}
	public void setAppid(String appid)
	{
		this.appid = appid;
	}
}
